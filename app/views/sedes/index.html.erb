<%= link_to "Exportar XML", sedes_path(:format => 'xml')%>

<h1>Listing sedes</h1>

<table>
  <tr>
    <th>Nombre</th>
    <th>Ciudad</th>
    <th>Universidad</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @sedes.each do |sede| %>
  <tr>
    <td><%= sede.nombre %></td>
    <td><%= sede.ciudad %></td>
    <td><%= sede.universidad.nombre %></td>
    <td><%= link_to 'Show', sede %></td>
    <td><%= link_to 'Edit', edit_sede_path(sede) %></td>
    <td><%= link_to 'Destroy', sede, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

